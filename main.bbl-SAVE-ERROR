      \strng{namehash}{f6964ada44364cdd3b1e0f8947107e6c}
      \strng{fullhash}{35d1be40a36590efe96644ba097f820f}
      \strng{bibnamehash}{218933dedbed86ff2cfa9870c361b180}
      \strng{authorbibnamehash}{218933dedbed86ff2cfa9870c361b180}
      \strng{authornamehash}{f6964ada44364cdd3b1e0f8947107e6c}
      \strng{authorfullhash}{35d1be40a36590efe96644ba097f820f}
      \strng{nameabibnamehash}{218933dedbed86ff2cfa9870c361b180}
